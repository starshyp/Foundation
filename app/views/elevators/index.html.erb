<p id="notice"><%= notice %></p>

<h1>Elevators</h1>

<table>
  <thead>
    <tr>
      <th>Serialnumber</th>
      <th>Model</th>
      <th>Elevatortype</th>
      <th>Status</th>
      <th>Dateofcommissioning</th>
      <th>Dateoflastinspection</th>
      <th>Certificateofinspection</th>
      <th>Info</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @elevators.each do |elevator| %>
      <tr>
        <td><%= elevator.SerialNumber %></td>
        <td><%= elevator.Model %></td>
        <td><%= elevator.ElevatorType %></td>
        <td><%= elevator.Status %></td>
        <td><%= elevator.DateOfCommissioning %></td>
        <td><%= elevator.DateOfLastInspection %></td>
        <td><%= elevator.CertificateOfInspection %></td>
        <td><%= elevator.Info %></td>
        <td><%= elevator.Notes %></td>
        <td><%= link_to 'Show', elevator %></td>
        <td><%= link_to 'Edit', edit_elevator_path(elevator) %></td>
        <td><%= link_to 'Destroy', elevator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Elevator', new_elevator_path %>
