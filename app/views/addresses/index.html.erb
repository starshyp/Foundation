<p id="notice"><%= notice %></p>

<h1>Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Typeofaddress</th>
      <th>Status</th>
      <th>Entity</th>
      <th>Numberandstreet</th>
      <th>Apt</th>
      <th>City</th>
      <th>Postalcode</th>
      <th>Country</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @addresses.each do |address| %>
      <tr>
        <td><%= address.TypeOfAddress %></td>
        <td><%= address.Status %></td>
        <td><%= address.Entity %></td>
        <td><%= address.NumberAndStreet %></td>
        <td><%= address.Apt %></td>
        <td><%= address.City %></td>
        <td><%= address.PostalCode %></td>
        <td><%= address.Country %></td>
        <td><%= address.Notes %></td>
        <td><%= link_to 'Show', address %></td>
        <td><%= link_to 'Edit', edit_address_path(address) %></td>
        <td><%= link_to 'Destroy', address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address', new_address_path %>
