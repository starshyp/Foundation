<p id="notice"><%= notice %></p>

<h1>Buildings</h1>

<table>
  <thead>
    <tr>
      <th>Fullnameofthebuildingadministrator</th>
      <th>Emailoftheadministratorofthebuilding</th>
      <th>Phonenumberofthebuildingadministrator</th>
      <th>Fullnameofthetechcontactforthebuilding</th>
      <th>Techcontactemail</th>
      <th>Techcontactphone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buildings.each do |building| %>
      <tr>
        <td><%= building.FullNameOfTheBuildingAdministrator %></td>
        <td><%= building.EmailOfTheAdministratorOfTheBuilding %></td>
        <td><%= building.PhoneNumberOfTheBuildingAdministrator %></td>
        <td><%= building.FullNameOfTheTechContactForTheBuilding %></td>
        <td><%= building.TechContactEmail %></td>
        <td><%= building.TechContactPhone %></td>
        <td><%= link_to 'Show', building %></td>
        <td><%= link_to 'Edit', edit_building_path(building) %></td>
        <td><%= link_to 'Destroy', building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

